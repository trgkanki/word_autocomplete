{
  "name": "addon_template",
  "version": "0.1.0",
  "description": "template for trgkanki's addon",
  "scripts": {
    "dev:js": "webpack -w",
    "build:js": "webpack",
    "build": "npm-run-all build:js pack",
    "pack": "cd src && bestzip ../dist.zip * && cd .."
  },
  "keywords": [
    "anki",
    "anki-addon"
  ],
  "author": "trgk",
  "license": "AGPL3",
  "devDependencies": {
    "@types/jquery": "^3.3.34",
    "@types/webpack-env": "^1.15.1",
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "bestzip": "^2.1.5",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.2.5",
    "jquery": "^3.5.0",
    "lint-staged": "^10.1.3",
    "npm-auto-version": "^1.0.0",
    "npm-run-all": "^4.1.5",
    "ts-loader": "^6.2.2",
    "typescript": "^3.8.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-env": "^0.8.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix"
    ],
    "*.{py}": [
      "autopep8 -i"
    ]
  },
  "dependencies": {}
}
